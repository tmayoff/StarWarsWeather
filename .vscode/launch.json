	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch via NPM",
			"runtimeExecutable": "npm",
			"env": {
				"DEBUG": "starwarsweather:server"
			},
			"runtimeArgs": [
				"--inspect-brk=5858",
				"run-script",
				"start"
			],
			"port": 5858,
			"skipFiles": [
				"<node_internals>/**"
			]
		}
		
	]
}